pragma solidity >=0.5.0 <0.9.0;
// Creating a Smart Contract
contract Structrecord{
// Structure of employee
struct allocrecord{
	// State variables
    uint bid;
	string recordid;
	string userid;
	string date;
	string record;
}
allocrecord []emps;
// Function to add
// addrecords details
function addrecord(uint bid, string memory recordid,string memory userid,string memory date,string memory record) public{
	allocrecord memory e =allocrecord(bid,recordid,userid,date,record);
	emps.push(e);
}
}
